    "playwright": "^1.57.0",
    "postcss": "^8.4.38",
    "prisma": "^5.14.0",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^4.0.17",
    "wait-on": "^8.0.3"
